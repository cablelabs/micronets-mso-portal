{
  "host": "localhost",
  "port": 3030,
  "public": "../public/",
  "src": "../src/",
  "paginate": {
    "default": 10,
    "max": 50
  },
  "mongodb": "mongodb://localhost:27017/mso-portal",
  "authentication": {
    "secret": "",
    "strategies": [
      "jwt"
    ],
    "path": "/authentication",
    "header": "Authorization",
    "service": "users",
    "entity": "users",
    "session": false,
    "cookie": {
      "enabled": false,
      "name": "feathers-jwt",
      "httpOnly": true,
      "secure": true
    },
    "jwt": {
      "header": {
        "type": "access"
      },
      "audience": "micronets.com",
      "subject": "Initiate registration to on-board device",
      "issuer": "https://mso-portal.micronets.com",
      "algorithm": "HS256",
      "expiresIn": "1d"
    }
  }
}
